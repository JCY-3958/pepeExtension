{
  "manifest_version": 3,
  "name": "Copy pepe",
  "description": "Copy pepe Faster",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/pepe.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": ["notifications"]
}

